summary(p2);AIC(p2);logLik(p2);Anova(p2)
# cell area
p3<-lmer(cell_area_delta ~ temperature + (1|stock),REML = FALSE, data=dat)
summary(p3);AIC(p3);logLik(p3);Anova(p3)
#OXYGEN ALONE
