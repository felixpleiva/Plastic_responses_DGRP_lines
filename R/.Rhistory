dat$sex<-as.factor(dat$sex)
dat$ID_fly<-as.factor(dat$ID_fly)
#model
m<-lmer(fresh_mass_log ~ (temperature + oxygen) * cell_area_log + sex + (1|stock),data=dat)
summary(m)
